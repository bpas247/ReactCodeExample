{
  "name": "react-code-example",
  "version": "2.0.0",
  "description": "Simple React application showcasing Redux and Jest (now using hooks!)",
  "private": true,
  "dependencies": {
    "immer": "^4.0.1",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-redux": "7.1.1",
    "react-router-dom": "5.1.2",
    "react-scripts": "3.2.0",
    "recharts": "^1.7.1",
    "redux": "4.0.4",
    "redux-starter-kit": "^0.7.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.1.2",
    "@testing-library/react": "^9.3.0",
    "husky": "^3.0.8",
    "prettier": "1.18.2",
    "pretty-quick": "^1.11.1",
    "redux-devtools": "3.5.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/node_modules/**",
      "!./src/registerServiceWorker.js",
      "!./src/index.js"
    ]
  }
}
