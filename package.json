{
  "name": "react-code-example",
  "version": "2.0.0",
  "description": "Simple React application showcasing Redux and Jest (now using hooks!)",
  "private": true,
  "dependencies": {
    "immer": "^9.0.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "7.2.4",
    "react-router-dom": "5.2.0",
    "react-scripts": "4.0.3",
    "recharts": "^2.0.10",
    "redux": "4.1.0",
    "redux-starter-kit": "^2.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "husky": "^6.0.0",
    "prettier": "2.3.2",
    "pretty-quick": "^3.1.1",
    "redux-devtools": "3.7.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/node_modules/**",
      "!./src/registerServiceWorker.js",
      "!./src/index.js"
    ]
  }
}
